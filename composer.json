{
    "name": "ingoldsby/laravel-dashboard-chuck-norris-joke-tile",
    "description": "A Chuck Norris joke tile for Laravel Dashboard",
    "keywords": [
        "ingoldsby",
        "laravel-dashboard-chuck-norris-joke-tile"
    ],
    "homepage": "https://github.com/ingoldsby/laravel-dashboard-chuck-norris-joke-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Ingoldsby",
            "email": "ingoldsby@ingoldsby.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ingoldsby\\ChuckNorrisJokeTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ChuckNorrisJokeTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ingoldsby\\ChuckNorrisJokeTile\\ChuckNorrisJokeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
